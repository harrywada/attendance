<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <title>Attendance</title>
  </head>
  <body>
    <form method="post" action="/">
      <input type="text" name="name" placeholder="Name" />
      <input type="submit" name="submit" value="submit" />
    </form>
    <ul>
      <% students.forEach(function(student){ %>
        <li><%= student.name %> - <%= student.attendance %></li>
      <% }) %>
    </ul>
  </body>
</html>
